<?xml version="1.0"?>
<!--
/**
 * Module loads external content into a page in Magento
 *
 * Created for SkiEssentials.com - 2019 - Written by Jeffrey Siegel
 */
-->
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <contentintegration>
            <general>
                <enable_in_frontend>1</enable_in_frontend>
                <head_title>SkiEssentials - Content Integration</head_title>
            </general>
        </contentintegration>
    </default>
</config>